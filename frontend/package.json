    "precompile": "npm-run-all precompile:*",
    "precompile:parser": "deno run -A --unstable https://deno-scripts.s3.amazonaws.com/mdx-parser.js ..\\content\\",
    "precompile:format": "prettier --write ./src/auto-generated",
